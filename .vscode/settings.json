{
  "files.exclude": {
    ".cache-loader": true,
    ".vscode": true,
    "node_modules": true,
  },
  "npm-intellisense.importQuotes": "\"",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "emmet.showAbbreviationSuggestions": false,
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
}