  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "productName": "Auryo",
    "files": [
      "dist/**/*",
      "static/**/*",
      "node_modules/",
      "package.json",
      "!**/node_modules/**/{*.md,README,readme,test,__tests__,tests,.*,LICENSE,license,powered-test,example,examples,*.d.ts,Makefile}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}"
    ],
    "appId": "be.sneljo.auryo",
    "publish": [
      {
        "provider": "github",
        "owner": "Superjo149",
        "repo": "auryo"
      }
    ],
    "protocols": [
      {
        "name": "auryo",
        "role": "Viewer",
        "schemes": [
          "auryo"
        ]
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.music",
      "hardenedRuntime": true
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "category": "Audio",
      "target": [
        "deb",
        "rpm",
        "AppImage",
        "pacman"
      ]
    },
    "snap": {
      "useTemplateApp": false,
      "summary": "A SoundCloud client for your desktop",
      "buildPackages": [
        "execstack"
      ],
      "stagePackages": [
        "libasound2",
        "libgconf2-4",
        "libnotify4",
        "libnspr4",
        "libnss3",
        "libpcre3",
        "libpulse0",
        "libxss1",
        "libxtst6"
      ],
      "plugs": [
        "bluez",
        "browser-support",
        "gsettings",
        "browser-support",
        "gsettings",
        "home",
        "mount-observe",
        "network",
        "opengl",
        "pulseaudio",
        "unity7",
        "x11",
        "mpris"
      ],
      "category": "Audio"
    },
    "directories": {
      "buildResources": "config/build",
      "output": "release"
    }
  },
  "devEngines": {
    "node": ">=8.10",
    "npm": ">=5.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "resolutions": {
    "**/**/nan": "^2.9.2"
  }
}
